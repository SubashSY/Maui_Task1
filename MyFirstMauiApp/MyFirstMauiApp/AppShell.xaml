<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyFirstMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyFirstMauiApp"
    Shell.FlyoutBehavior="Flyout"
    Title="MyFirstMauiApp">
    <TabBar>
        <Tab Title="Home" Icon="dotnet_bot.png">
            <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        </Tab>
        <Tab Title="Layout Example"  Icon="dotnet_bot.png">
            <ShellContent
        Title="Layout Example"
        ContentTemplate="{DataTemplate local:LayoutExample}"
        Route="LayoutExample" />
        </Tab>
        
    </TabBar>


</Shell>
